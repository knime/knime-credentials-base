<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.credentials.base.CredentialType">
      <credentialType
            credentialClass="org.knime.credentials.base.oauth.api.JWTCredential"
            credentialSerializerClass="org.knime.credentials.base.oauth.api.JWTCredential$Serializer"
            id="org.knime.credentials.base.oauth.api.JWTCredential"
            name="JWT Credential">
      </credentialType>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/io/connectors"
            deprecated="false"
            factory-class="org.knime.credentials.base.oauth.node.generic.GenericOAuthAuthenticatorNodeFactory"
            hidden="false">
      </node>
      <node
            category-path="/io/connectors"
            deprecated="false"
            factory-class="org.knime.credentials.base.oauth2.password.OAuth2AuthenticatorPasswordNodeFactory"
            hidden="false">
      </node>
      <node
            category-path="/io/connectors"
            deprecated="false"
            factory-class="org.knime.credentials.base.oauth2.clientcredentials.OAuth2AuthenticatorClientCredsNodeFactory"
            hidden="false">
      </node>
   </extension>
</plugin>
